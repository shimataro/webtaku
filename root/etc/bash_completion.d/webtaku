# webtaku(1) completion

function __webtaku_complete_wordlist()
{
	cat <<WORDLIST
--format=
--output=
--accept-language=
--cookie=
--user-agent=
--min-size=
--scaled-size=
--timer=
--crop 
--silent 
--version 
WORDLIST
}

function __webtaku_complete()
{
	local wordlist=`__webtaku_complete_wordlist`
	local current=${COMP_WORDS[COMP_CWORD]}

	local IFS=$'\n'
	COMPREPLY=(`compgen -W "${wordlist}" -- "${current}"`)
}

complete -o nospace -F __webtaku_complete webtaku
